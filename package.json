{
  "name": "reckon-ai-indexer",
  "version": "1.0.0",
  "description": "Enterprise-Grade Blockchain Data Platform with AI-Powered Analytics",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t reckon-ai-indexer .",
    "docker:run": "docker run -p 3001:3001 reckon-ai-indexer",
    "db:migrate": "ts-node src/database/migrations/run-migrations.ts",
    "db:seed": "ts-node src/database/seeds/run-seeds.ts"
  },
  "keywords": [
    "blockchain",
    "oracle",
    "ai",
    "defi",
    "price-feeds",
    "chainlink",
    "pyth",
    "bittensor",
    "web3",
    "cryptocurrency"
  ],
  "author": "Reckon AI Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@chainlink/contracts": "^1.2.0",
    "@pythnetwork/client": "^2.22.1",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "child_process": "^1.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.1.0",
    "ethers": "^5.8.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-ws": "^5.0.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-postgres": "^0.6.2",
    "pg": "^8.16.3",
    "redis": "^5.6.0",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.1.3",
    "timescale": "^0.1.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-ws": "^3.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.12",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
